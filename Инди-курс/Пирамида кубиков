n = int(input())  # Вводим значение n

level = 0         # Текущий уровень пирамиды
level_kub = 0     # Сумма кубов уровней
s = 0             # Общая сумма кубов всех уровней

# Цикл продолжается, пока сумма кубов всех уровней меньше n
while s < n:
    level += 1        # Увеличиваем уровень на 1
    level_kub += level  # Добавляем новый уровень к сумме уровней
    s += level_kub    # Добавляем сумму кубов текущего уровня к общей сумме

# Если сумма ровно равна n, выводим текущий уровень, иначе на один меньше
print(level if s == n else level - 1)
